  "devDependencies": {
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "concurrently": "^8.2.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.4"
  },
  "dependencies": {
    "autoprefixer": "^10.4.21",
    "lucide-react": "^0.542.0",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^3.4.17"
  }
